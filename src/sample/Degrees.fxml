<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Degrees">
   <children>
      <AnchorPane prefHeight="85.0" prefWidth="600.0" style="-fx-background-color: #000051;">
         <children>
            <Label alignment="CENTER" layoutX="143.0" layoutY="14.0" prefHeight="39.0" prefWidth="314.0" text="Goodwill Institute" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="34.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="275.0" layoutY="53.0" text="Degrees" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <JFXButton fx:id="back_btn" layoutX="10.0" layoutY="14.0" onAction="#HandleBtn" prefHeight="27.0" prefWidth="50.0" style="-fx-background-color: White;" text="Back" />
            <JFXButton fx:id="exit_btn" layoutX="10.0" layoutY="48.0" onAction="#HandleBtn" prefHeight="27.0" prefWidth="50.0" style="-fx-background-color: White;" text="Exit" />
         </children>
      </AnchorPane>
      <TableView fx:id="tbl" layoutX="-1.0" layoutY="85.0" prefHeight="314.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="deg_Col" prefWidth="310.0" text="Degree" />
            <TableColumn fx:id="year_Col" minWidth="0.0" prefWidth="112.0" text="Years of Study" />
            <TableColumn fx:id="fac_Col" prefWidth="177.0" text="Faculty" />
         </columns>
